		"prettier": "3.3.2",
		"prettier-plugin-svelte": "3.2.5",
		"prettier-plugin-tailwindcss": "0.6.5",
		"svelte": "4.2.18",
		"svelte-check": "3.8.4",
		"tailwindcss": "3.4.4",
		"tslib": "2.6.3",
		"typescript": "5.5.3",
		"vite": "5.3.3"
